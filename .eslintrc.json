{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off",
        "comma-dangle": 0,
        "linebreak-style": 0,
        "prefer-destructuring": 0,
        "no-plusplus": 0,
        "import/newline-after-import": 0,
        "arrow-body-style": "off",
        "arrow-parens": 0,
        "no-await-in-loop": 0,
        "class-methods-use-this": 0,
        "dot-notation": "off",
        "object-curly-newline": "off"
    }
}